@article{LI2022114531,
title = {Non-neglectable therapeutic options for age-related macular degeneration: A promising perspective from traditional Chinese medicine},
journal = {Journal of Ethnopharmacology},
volume = {282},
pages = {114531},
year = {2022},
issn = {0378-8741},
doi = {https://doi.org/10.1016/j.jep.2021.114531},
