@incollection{MITROPOULOU202387,
title = {Chapter 6 - Economic evaluation of personalized medicine interventions in medium- and low-income countries with poor proliferation of genomics and genetic testing},
editor = {Christina Mitropoulou and Sarah Wordsworth and James Buchanan and George P. Patrinos},
booktitle = {Economic Evaluation in Genomic and Precision Medicine},
publisher = {Academic Press},
pages = {87-98},
year = {2023},
series = {Translational and Applied Genomics},
isbn = {978-0-12-813382-8},
doi = {https://doi.org/10.1016/B978-0-12-813382-8.00004-5},
url = {https://www.sciencedirect.com/science/article/pii/B9780128133828000045},
author = {Christina Mitropoulou and George P. Patrinos},
keywords = {Pharmacogenomics, Genomic and personalized medicine, Cost-effectiveness analysis, Economic evaluation, Medium- and low-income economies},
abstract = {There are currently calls for the clinical application of personalized medicine worldwide. Yet, economic evaluation data, if available, are scarce and the availability of such data is even more limited for medium- and low-income countries, especially those with poor proliferation of genomics and genetic testing services. As such, there is wide variation in the clinical application of personalized medicine between low- and high-income countries. In this chapter, we review economic evaluation studies conducted in medium- and low-income countries around the world, especially those with poor proliferation of genomics and genetic testing services. Using high-income European countries as a model of clinical application, we find that economic evaluation studies in pharmacogenomics are still in their infancy for the majority of the medium- and low-income countries European countries. The examples presented suggest that economic evaluation studies will need to be adapted and replicated in every country for policymakers to be fully informed prior to the implementation of a pharmacogenomic-guided medical intervention. Given this, generic economic models are required that allow key variables, such as allele frequency and test and treatment costs, to vary by country. Such models are also discussed herein.}
}
