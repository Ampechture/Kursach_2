@article{LI2023127855,
title = {Bioceramic calcium phosphate-polymer scaffolds: A promising strategy for osteochondral repair and regenerative medicine},
journal = {Materials Chemistry and Physics},
volume = {304},
pages = {127855},
year = {2023},
issn = {0254-0584},
doi = {https://doi.org/10.1016/j.matchemphys.2023.127855},
url = {https://www.sciencedirect.com/science/article/pii/S0254058423005631},
author = {Xiaohua Li and Bahareh Noshadi and Kiana Motamedi and Emad Movahed and Pouya Behfarnia and D.T. Semiroumi},
keywords = {Porous bony scaffold, Freeze-drying, Nanocomposite, Bioceramic, Osteochondral},
abstract = {The study's objective was to fabricate a porous scaffold with desirable biological and bioactive characteristics, utilizing freeze-drying with polyvinyl alcohol (PVA). Subsequently, compressive strength tests and simulated body fluid (SBF) soaking experiments were performed to assess the mechanical and biological properties of the scaffold. X-ray diffraction (XRD) and scanning electron microscopy (SEM) were used for analyzing the scaffolds. The findings indicated that an increase in the amount of dicalcium phosphate decreased the porosity of the scaffold from 76% to 64%, raised the compressive strength from 2.48 MPa to 4.8 MPa, and lowered the dissolution rate from 39% to 24%. These changes led to an improvement in the scaffold's chemical bonding and stability. According to micromechanical models, the optimal scaffold composition was found to be 10 wt% of dicalcium phosphate in a polyvinyl alcohol mixture with constant drug content. Moreover, an increase in the dicalcium phosphate content led to an elevation in the drug release percentage due to the reduction in porosity and an increase in the drug release rate. The data were analyzed by using various kinetic models through diagrams generated by a UV device.}
}
@article{ORBE2023100556,
title = {Towards biomarker-based personalized medicine in psychiatry: an individual-level participant data meta-analysis of anti-inflammatory add-on treatment for psychotic disorders},
journal = {Journal of Affective Disorders Reports},
volume = {12},
pages = {100556},
year = {2023},
issn = {2666-9153},
doi = {https://doi.org/10.1016/j.jadr.2023.100556},
url = {https://www.sciencedirect.com/science/article/pii/S2666915323000951},
author = {Elif B. Orbe},
abstract = {Objective
We have previously conducted a meta-analysis of all randomized control trials investigating the antipsychotic effect of immunomodulating drugs in individuals with psychotic disorders which showed an overall improvement of psychopathology; however, investigations are lacking on individuals with elevated baseline inflammatory biomarkers, who might have a larger beneficial effect of immunomodulating drugs compared to those without increased inflammation.
Method
Multiple databases were searched up until January 2022. Only double-blinded, randomized, placebo-controlled clinical trials (RCTs) were included. All authors were contacted in regards of sharing individual-level participant data including psychopathology at baseline and end-of-treatment and at least one type of biomarker at baseline. The primary outcome is whether anti-inflammatory add-on treatment results in a greater decrease in psychopathology in those with higher levels of any inflammatory biomarker, compared to those with lower levels. Secondary outcomes include an analysis of each of the individual inflammatory biomarkers one by one, whether the efficacy of anti-inflammatory add-ons is greater in those with higher levels hereof, and an investigation of whether anti-inflammatory add-on treatment decreases levels of these inflammatory biomarkers over time. We will preserve the clustering of patients within studies and thus analyse the data in a two-step approach. Firstly, we will estimate a standardised mean difference (SMD) for each individual study. Secondly, the data from each individual study will be synthesized using the random-effects model and fixed-effect model meta-analyses.
Results
Will be presented at the conference
Conclusions
The Results will provide useful knowledge for future work towards biomarker-based personalized medicine in psychiatry.}
}
