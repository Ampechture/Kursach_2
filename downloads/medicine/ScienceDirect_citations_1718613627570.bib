@article{JOHNSON2022782,
title = {An Internist, a Surgeon, and a Psychiatrist Walk into a Hospice and Palliative Medicine Fellowship: An Early and Sequential Simulation Curriculum for Individualized Fellow Learning (TH108)},
journal = {Journal of Pain and Symptom Management},
volume = {63},
number = {5},
pages = {782-783},
year = {2022},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2022.02.201},
url = {https://www.sciencedirect.com/science/article/pii/S088539242200286X},
author = {Sara Johnson and Cassie Shumway and Rhiannon Talbot and Kathryn Schueller},
abstract = {Outcomes 1. Discuss the current state, benefits, and future directions of simulation-based medical education (SBME) in hospice and palliative medicine (HPM) 2. Examine SBME within the theoretical framework of the development of expertise and the use of simulation in HPM education for individualized learning and practice 3. Outline key resources and best practices in high-fidelity SBME planning and implementation, including case development, prebrief and debrief activities, standardized patient preparation, and faculty development. Development of expertise requires deliberate practice, which necessitates observation, high-level formative feedback, and repetition. Hospice and palliative medicine (HPM) fellowships aim, in one short year, to increase fellow expertise in caring for seriously ill patients. HPM fellows hail from various specialty backgrounds, contributing to diverse learning needs; early identification of individual needs is imperative to optimize learning. Simulation-based medical education (SBME) can be used as both a teaching and formative assessment method in navigating complex, urgent clinical situations. Our HPM fellowship clinical case simulation curriculum aims to inform fellows’ learning needs and individualized learning plans; provide ongoing formative feedback at fellows’ zones of proximal development in common, high-stakes HPM scenarios (acute cancer pain crisis and imminent end-of-life care); and allow for deliberate practice with escalation of case complexity throughout fellowship. To incorporate best practices for SBME curricular development, we created a prebrief process, checklist of desired learner actions, and a debrief framework. Summaries of interprofessional educator formative feedback and fellow self-assessments are cultivated to develop short-term learning goals for fellows’ individualized learning plans (ILPs). Cases are repeated mid-year with adjustment in challenge to build on foundational skills, again culminating in updated ILPs. We developed late-year, sequential simulations of the same cases at future points (refractory cancer pain and withdrawal of respiratory support) to extend learning. Fellows report that simulations promote perceptions of a safe learning environment and that ILPs are particularly helpful to crystallize learning goals and steps early in the fellowship. Checklists of observable skills, formative feedback summaries, and fellow self-assessments reveal consistent improvement in fellows’ development, which supports competency in related milestones. This curriculum also provides an opportunity to develop interprofessional HPM educator teaching skills. Expansion of SMBE in HPM education can be used to identify and meet the spectrum of fellows’ individual learning needs.}
}
@article{RIPOLLESMELCHOR2023319,
title = {Research priorities in perioperative fluid therapy and hemodynamic monitoring: A Delphi Consensus Survey from the Fluid Therapy and Hemodynamic Monitoring Subcommittee of the Hemostasis, Transfusion Medicine and Fluid Therapy Section (SHTF) of the Spanish Society of Anesthesiology and Critical Care (SEDAR)},
journal = {Revista Española de Anestesiología y Reanimación (English Edition)},
volume = {70},
number = {6},
pages = {319-326},
year = {2023},
issn = {2341-1929},
doi = {https://doi.org/10.1016/j.redare.2022.04.003},
url = {https://www.sciencedirect.com/science/article/pii/S2341192923001038},
author = {J. Ripollés-Melchor and M.J. Colomina and C. Aldecoa and J. Alonso-Cabello and J.M. Alonso-Íñigo and H. Aya and M. Basora and F. Clau-Terre and E. {del Cojo-Peces} and F. Cota-Delgado and R. Ferrandis-Comes and P. Galán-Menéndez and D. García-López and I. Garruti and I.J. López and J.L. Jover-Pinillos and J.V. Llau-Pitarch and J.V. Lorente and J. Mesquida and I. Mojarro and M.I. Monge-García and S.C. Montesinos-Fadrique and J.L. Muñoz-Rodes and M. {de Nadal} and F. Ramasco and J.L. Tomé-Roca and A. Pérez and R. Uña-Orejón and G. Yanes and A. Zorrilla-Vaca and D. Escarraman and J. García-Fernández},
keywords = {Fluid therapy, Hemodynamics, Research, Fluidoterapia, Hemodinámica, Investigación},
abstract = {Background
Research in fluid therapy and perioperative hemodynamic monitoring is difficult and expensive. The objectives of this study were to summarize these topics and to prioritize these topics in order of research importance.
Methods
Electronic structured Delphi questionnaire over three rounds among 30 experts in fluid therapy and hemodynamic monitoring identified through the Fluid Therapy and Hemodynamic Monitoring Subcommittee of the Hemostasis, Transfusion Medicine and Fluid Therapy Section of the Spanish Society of Anesthesiology and Critical Care.
Results
77 topics were identified and ranked in order of prioritization. Topics were categorized into themes of crystalloids, colloids, hemodynamic monitoring and others. 31 topics were ranked as essential research priority. To determine whether intraoperative hemodynamic optimization algorithms based on the invasive or noninvasive Hypotension Prediction Index versus other management strategies could decrease the incidence of postoperative complications. As well as whether the use of renal stress biomarkers together with a goal-directed fluid therapy protocol could reduce hospital stay and the incidence of acute kidney injury in adult patients undergoing non-cardiac surgery, reached the highest consensus.
Conclusions
The Fluid Therapy and Hemodynamic Monitoring Subcommittee of the Hemostasis, Transfusion Medicine and Fluid Therapy Section of the Spanish Society of Anesthesiology and Critical Care will use these results to carry out the research.
Resumen
Antecedentes
La investigación sobre fluidoterapia y monitorización hemodinámica perioperatorias es difícil y costosa. Los objetivos del presente estudio fueron resumir y priorizar estas cuestiones, en orden de importancia investigadora.
Métodos
Cuestionario estructurado electrónico Delphi a lo largo de tres rondas entre 30 expertos en fluidoterapia y monitorización hemodinámica identificados a través del Subcomité de Fluidoterapia y Monitorización hemodinámica de la Sección de Hemostasis, Medicina Transfusional y Fluidoterapia de la Sociedad española de Anestesiología y Reanimación.
Resultados
Se identificaron 77 cuestiones, que se clasificaron en orden de priorización. Las cuestiones se categorizaron en temas de cristaloides, coloides, monitorización hemodinámica y otros. Se categorizaron 31 cuestiones como de prioridad investigadora esencial, para determinar si los algoritmos de optimización hemodinámica intraoperatorios basados en el Índice de predicción de hipotensión invasivo o no invasivo frente a otras estrategias de manejo podrían reducir la incidencia de complicaciones postoperatorias, así como si el uso de biomarcadores del estrés renal junto con un protocolo de fluidoterapia dirigido por objetivos podría reducir la estancia hospitalaria y la incidencia de insuficiencia renal aguda en pacientes adultos sometidos a cirugía no cardiaca, lográndose el más alto consenso.
Conclusiones
El Subcomité de Fluidoterapia y Monitorización hemodinámica de la Sección de Hemostasis, Medicina Transfusional y Fluidoterapia de la Sociedad española de Anestesiología y Reanimación utilizará estos resultados para la realización de investigación.}
}
