@incollection{ROSENBERG2023157,
title = {Chapter 9 - Machine learning and precision medicine},
editor = {Gary A. Rosenberg},
booktitle = {Neuroinflammation in Vascular Dementia},
publisher = {Academic Press},
pages = {157-173},
year = {2023},
