issn = {0065-2806},
doi = {https://doi.org/10.1016/bs.aiip.2023.09.002},
url = {https://www.sciencedirect.com/science/article/pii/S0065280623000164},
author = {Richard H. ffrench-Constant},
keywords = {Bacterial toxin, , , Insecticide, Toxin complex, Tc, Mcf, PVC, Protein delivery, Secretion system},
abstract = {Bacterial toxins from the insect pathogenic bacterium Photorhabdus were originally studied for their potential use in insect-resistant genetically modified crops. However, over the last decade, it has become increasingly clear that the large and complex array of toxins made by this insect pathogen are also useful as novel protein delivery systems. In this review, the orally active Toxin complex proteins, or Tcs, are described as unique syringe-like delivery systems, with a well described receptor, Visgun or Vsg. The Photorhabdus Virulence Cassettes or PVCs are unique phage-like delivery systems, that have recently been re-engineered to deliver their toxic cargoes into specific cell types for use in medicine. The Makes Caterpillars Floppy, or Mcf-like, toxins are large multi-domain toxins whose structure and function is now finally becoming clear, with Mcf-like domains being found in a range of agriculturally and medically important bacteria. Therefore, what began as a simple search for orally active toxins to use as alternatives to those from Bacillus thuringiensis, has now become a field relevant to both medicine and agriculture with applications ranging from targeted protein delivery into cancer cells, to biological control in the rhizosphere.}
}
@article{CHEN2022100756,
