@article{KUO2022105828,
title = {Towards precision sleep medicine: Self-attention GAN as an innovative data augmentation technique for developing personalized automatic sleep scoring classification},
journal = {Computers in Biology and Medicine},
volume = {148},
pages = {105828},
year = {2022},
issn = {0010-4825},
doi = {https://doi.org/10.1016/j.compbiomed.2022.105828},
url = {https://www.sciencedirect.com/science/article/pii/S001048252200587X},
author = {Chih-En Kuo and Tsung-Hua Lu and Guan-Ting Chen and Po-Yu Liao},
keywords = {Personalized automatic sleep scoring classification, Self-attention generative adversarial networks, Convolution neural network, Data augmentation, Sleep EEG spectrogram},
abstract = {It is very important to have good quality sleep, which can affect aspects such as memory consolidation, emotional regulation, learning, physical development, and quality of life. Diagnosing human sleep quality and problems quickly and accurately is an important issue for human well-being. Therefore, many automatic sleep scoring methods have been proposed. However, the methods have been developed using sleep data from different individuals or groups. The accuracies of these proposed methods might decrease, due to existing individual differences. In this study, the self-attention generative adversarial network (SAGAN) was applied as an advanced data augmentation technique to propose an improved personalized automatic sleep scoring classification. First, the spectrograms were converted from electroencephalography (EEG). Then, SAGAN was used to generate synthesized spectrograms for each subject. Finally, the real and synthesized spectrograms of each subject were utilized to train a personalized classifier. The averaged accuracy and standard deviation of the proposed method are 95.74% and 3.78%, respectively. Compared to the classifier trained with all subjects’ training data, the average accuracy increased by 8.08%. The results proved that the generated spectrograms significantly improved the performance of the personalized automatic sleep scoring classification. The contributions of the proposed method were that made the medical staff and subjects save massive medical resources and time for manual recording and scoring.}
}
@article{LIANTONIO2022814,
title = {Creating LGBTQ+ Competencies for Hospice and Palliative Medicine (HPM) Fellowship Programs (FR238)},
journal = {Journal of Pain and Symptom Management},
volume = {63},
number = {5},
pages = {814-815},
year = {2022},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2022.02.265},
url = {https://www.sciencedirect.com/science/article/pii/S0885392422003505},
author = {John Liantonio and Corey Tapper and Elizabeth Spina and Michael Danielewicz and Noelle Marie Javier},
abstract = {Outcomes 1. Develop a proposal for standardized guidelines and appropriate metrics for LGBTQ+ competencies in hospice and palliative medicine fellowship 2. Identify mechanisms to create safe and brave spaces for discussions about diversity and inclusion topics during training and in practice 3. Review, describe, and analyze gaps in legal and medical systems for protecting LGBTQ+ people at the end of life The field of hospice and palliative medicine (HPM) is increasingly recognizing the importance of care that is respectful and attentive to gender and sexual orientation. LGBTQ+ people face several unique health disparities that may affect their care at the end of life. Some may not feel comfortable disclosing their LGBTQ+ identity to providers, either in an office or a hospice setting; studies have shown that LGBTQ+ patients feel less comfortable in their own homes with home care services involved, fearing discrimination.1 Frequently, palliative care follows a patient across multiple levels of care; however, care transitions can bring risk of unaccepting environments or poor patient-provider interactions. In some cases, it has been documented that patients feel they are pushed “back into the closet” due to actual or feared bigotry.2 Support networks differ as well, as many LGBTQ+ individuals rely on families of choice. If these support systems are not appropriately identified or there is lack of adequate documentation, these chosen families may fail to be recognized as decision makers under state laws.3 Transgender individuals are uniquely vulnerable, relying on others to honor their gender identity at the end of life and may face violation of that gender identity by being misgendered.4 Lack of education in LGBTQ+ palliative and end-of-life care in HPM fellowship programs can theoretically create further healthcare disparities. Education should emphasize the unique challenges that LGBTQ+ people often face, sensitive and open dialogue, extension of support to chosen families, and means to ensure wishes are carried forward. This session will serve as a workshop for fellowship, academic, and community-based hospice and palliative care programs to help create and develop specific LGBTQ+ educational competences that can be presented to the academy to provide guidance for training and clinical programs to ensure high-quality palliative care for LGBTQ+ people living with serious illness.}
}
