keywords = {Electrospinning/air-blowing technique, Cotton-like poly(ε-caprolactone) microfibers, Collagen hydrogel/nanofibers, Large-volume porous scaffold, Large-volume tissue defect, Regenerative medicine},
abstract = {The fabrication of engineered scaffolds for the regeneration of large-volume tissue defects remains a major challenge in regenerative medicine. Herein, we report a novel fabrication of a novel large-volume and highly porous scaffold of poly(ε-caprolactone) microfibers/collagen nanofibers (PCL/COL). Electrospinning/air-blowing technique was firstly used for production of PCL cotton-like fibers (PCLCLF). PCL/COL scaffold was then fabricated via mechanical compression of PCLCLF, infiltration of the compressed PCLCLF with hydrogel-forming COL solution and finally lyophilization. The large-volume PCL/COL porous scaffold demonstrated unique micro/nanofibrous architecture and highly porous structure, in addition to high water-uptake capacity, good cuttability and excellent punchability. Therefore, this large-volume PCL/COL porous scaffold can be potentially proposed for regeneration of non-load bearing/large-volume tissue defects.}
}
@article{CISEWSKI2022180,
title = {Emergency medicine updates: Droperidol},
journal = {The American Journal of Emergency Medicine},
volume = {53},
pages = {180-184},
year = {2022},
issn = {0735-6757},
doi = {https://doi.org/10.1016/j.ajem.2022.01.011},
url = {https://www.sciencedirect.com/science/article/pii/S0735675722000158},
author = {David Cisewski and Brit Long and Michael Gottlieb},
keywords = {Droperidol, Agitation, Pain, Nausea, Headache, vertigo, QT prolongation, Prolonged QT},
abstract = {Introduction
Droperidol is a butyrophenone that has recently been reintroduced after a United States Food and Drug Administration (US FDA) black box warning in 2001. Evidence demonstrates utility in a variety of clinical conditions.
Objective
This paper provides evidence-based updates concerning the use of droperidol for the emergency clinician.
Discussion
Droperidol received a black box warning by the US FDA in 2001 due to concerns for QT prolongation and torsades de pointes; however, reevaluation of the available data suggests droperidol is a safe and efficacious medication. It can be used in the emergency department (ED) setting for many conditions, including acute agitation, headaches, vertigo, nausea, and vomiting. Extensive literature supports that the QT-prolonging effects are transient and that the risk of torsades de pointes is rare with doses utilized in the ED. An electrocardiogram does not need to be routinely obtained before droperidol use but should be considered in patients at high risk for QT prolongation.
Conclusions
Current evidence suggests that droperidol is a safe and effective medication for treating nausea and vomiting, headache, vertigo, and agitation in the ED setting.}
}
