@article{ZEMPSKY2023387,
title = {Analgesic, Anesthetic, and Addiction Clinical Trial Translations, Innovations, Opportunities, and Networks−American Pain Society−American Academy of Pain Medicine Pain Taxonomy Diagnostic Criteria for Acute Needle Pain},
journal = {The Journal of Pain},
volume = {24},
