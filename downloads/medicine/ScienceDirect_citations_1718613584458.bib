@article{PAN2022100775,
title = {Chinese herbal medicine (Bu-Shen-Tian-Jing Formula) for outcomes of IVF in Chinese patients with polycystic ovary syndrome: A retrospective cohort study},
journal = {Integrative Medicine Research},
volume = {11},
number = {1},
pages = {100775},
