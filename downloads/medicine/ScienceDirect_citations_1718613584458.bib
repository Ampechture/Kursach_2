author = {M. Arbitrio and A. Cordua and V. Uppolo and M. D’Apolito and D. Caracciolo and N. Staropoli and O. Cuomo and L. Fiorillo and P. Tassone and M.T. {Di Martino} and P. Tagliaferri},
