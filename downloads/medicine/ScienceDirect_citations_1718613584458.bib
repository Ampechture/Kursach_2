@article{MOSS2023e578,
title = {"Wait, They're Dying?! Can I Practice First?!" Creation of a Novel Simulation-Based Palliative Care Curriculum for the Emergency Medicine Resident (Sch400)},
journal = {Journal of Pain and Symptom Management},
volume = {65},
number = {5},
pages = {e578},
year = {2023},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2023.02.137},
url = {https://www.sciencedirect.com/science/article/pii/S0885392423002166},
author = {Hillary Moss and Amir Rashed and Julien Menko and John Thomas and Bruce R. Gutierrez and Andrew Restivo},
abstract = {Outcomes
1. Utilizing simulation technology and patient actors, participants will learn basic communication skills utilized in palliative care, which can be applied in the emergency department setting. 2. Utilizing simulation technology and small group discussions, participants will be able to demonstrate improved knowledge in the management of common symptoms at the end of life.
Background
Palliative care (PC) improves quality of life for seriously ill patients nearing the end of life. Given that over 75% of older adults visit the ED during last 6 months of life, emergency medicine (EM) clinicians should have a robust understanding of PC. However, EM residents feel underprepared to utilize PC in their practice. To address this need, we created a simulation-based PC curriculum for the ED.
Methods
Key PC topics for EM were identified, and learning objectives and simulation cases/lectures were created. The curriculum was implemented in an EM residency with 84 residents who serve an ethnically and socioeconomically diverse patient population. Lectures and simulations on the same topics occurred on the same day, and residents were randomly assigned to simulations. A pre- and postintervention survey assessed resident comfort level and knowledge of PC topics.
Results
Seven PC topics were identified: goals-of-care (GOC) discussions, cancer complications, hospice care, pain management, communicating bad news, legal/ethical issues, and end-of-life symptom management. Presurvey data revealed 72.3% of residents agree/strongly agree that PC is important to emergency clinicians; 86.2% never/rarely consulted PC; 46.2% didn't feel comfortable giving death notification; 58.3% had been trained to give bad news. Advance directives were regularly discussed by 30.6%, and 11.1% assured accurate code status in the EMR. 80.6% of residents didn't feel comfortable having GOC conversations. Postsurvey data is still being analyzed.
Conclusion
Prior to the intervention, residents confirmed the importance of PC but reported discomfort in key areas of ED practice (death notification, GOC conversations). Utilizing simulation allowed for practice and feedback on communication skills required for these difficult conversations. Applied longitudinally, this curriculum may increase resident knowledge of PC and improve quality of life and mitigate suffering for patients near the end of life seen in emergency medicine practice.}
}
@article{CERISOLI20221760,
title = {Building a Healthcare Alliance for Resourceful Medicine Offensive Against Neoplasms in Hematology Added Value Framework for Hematologic Malignancies: A Comparative Analysis of Existing Tools},
journal = {Value in Health},
volume = {25},
number = {10},
pages = {1760-1767},
year = {2022},
issn = {1098-3015},
doi = {https://doi.org/10.1016/j.jval.2022.04.1729},
url = {https://www.sciencedirect.com/science/article/pii/S1098301522019313},
author = {Francesco Cerisoli and Farzad Ali and Tamás Bereczky and Natacha Bolaños and Lars Bullinger and Sujith Dhanasiri and James Gallagher and Sonia García Pérez and Jan Geissler and Yann Guillevic and Kathryn Harrison and Anastasia Naoum and Carla Portulano and Ana E. {Rodríguez Vicente} and Renate Schulze-Rath and Gabriela Yumi Gómez and Guillermo Sanz and Jesús María {Hernández Rivas}},
keywords = {clinical value framework, hematology, Healthcare Alliance for Resourceful Medicine Offensive Against Neoplasms in hematologY, patient-reported outcomes},
abstract = {Objectives
The Innovative Medicines Initiative–funded, multistakeholders project Healthcare Alliance for Resourceful Medicine Offensive Against Neoplasms in Hematology (HARMONY) created a task force involving patient organizations, medical associations, pharmaceutical companies, and health technology assessment/regulator agencies’ representatives to evaluate the suitability of previously established value frameworks (VFs) for assessing the clinical and societal impact of new interventions for hematologic malignancies (HMs).
Methods
Since the HARMONY stakeholders identified the inclusion of patients’ points of view on evaluating VFs as a priority, surveys were conducted with the patient organizations active in HMs and part of the HARMONY network, together with key opinion leaders, pharmaceutical companies, and regulators, to establish which outcomes were important for each HM. Next, to evaluate VFs against the sources of information taken into account (randomized clinical trials, registries, real-world data), structured questionnaires were created and filled by HARMONY health professionals to specify preferred data sources per malignancy. Finally, a framework evaluation module was built to analyze existing clinical VFs (American Society of Clinical Oncology, European Society of Medical Oncology, Magnitude of Clinical Benefit Scale, Institut für Qualität und Wirtschaftlichkeit im Gesundheitswesen, Institute for Clinical and Economic Review, National Comprehensive Cancer Network Evidence Blocks, and patient-perspective VF).
Results
The comparative analysis describes challenges and opportunities for the use of each framework in the context of HMs and drafts possible lines of action for creating or integrating a more specific, patient-focused clinical VF for HMs.
Conclusions
None of the frameworks meets the HARMONY goals for a tool that applies to HMs and assesses in a transparent, reproducible, and systematic way the therapeutic value of innovative health technologies versus available alternatives, taking a patient-centered approach and using real-world evidence.}
}
@article{WANG2024115859,
title = {Establishing an integrated, four-dimensional quality assessment system for traditional Chinese medicine: A case study of Shuanghuanglian oral liquid},
journal = {Journal of Pharmaceutical and Biomedical Analysis},
volume = {239},
pages = {115859},
year = {2024},
issn = {0731-7085},
doi = {https://doi.org/10.1016/j.jpba.2023.115859},
url = {https://www.sciencedirect.com/science/article/pii/S0731708523006283},
author = {Pengyue Wang and Xinyi Wang and Jiajia Fan and Yifang Li and Rongrong He and Jin Gao and Chengyu Chen and Huiqing DAI and Zhiming Cao and Lili Lan and GuoXiang Sun and Wanyang Sun},
keywords = {Electrochemistry, HPLC, GC, UV, Quality Consistency, Integrated Fingerprinting},
abstract = {The quality of traditional Chinese medicine (TCM) is the premise to ensure its safety and effectiveness in clinical application. In this study, a complete quality control system for four-dimensional fingerprinting of TCM was innovatively constructed based on multiple detection techniques, and the quality of Shuanghuanglian oral liquid (SHL) was evaluated. Electrochemical fingerprinting (ECFP) as an emerging method without pretreatment provides rich and quantifiable information for SHL samples. The first quantitative ECFP of SHL was developed by the B-Z oscillation system. Eight characteristic parameters were analyzed and a good linear relationship was found between the oscillation lifetime and sample volume, by which the calculated values of the added sample volume (VL) showed different fluctuations between samples. What is more, high-performance liquid chromatography five-wavelength fusion fingerprint (HPLC-FWFP), GC fingerprint (GC-FP), and UV quantum fingerprint (UV-QFP) was established. Meanwhile, the purity of the peaks of the HPLC-FWFP was verified by the dual-wavelength absorption coefficient ratio spectrum (DWAR). Equal weighted ratio quantitative fingerprinting method (EWRQFM) was successfully proposed to extract all potential features for the overall quality assessment of the samples. Finally, a comprehensive evaluation strategy was proposed, namely the variation coefficient weighting algorithm (VCWA). The results of qualitative and quantitative evaluation of HPLC-FWFP, GC-FP, electrochemical quantum fingerprints (EC-QFP), and UV-QFP were integrated by this method. The established evaluation system is also a suitable strategy to control the quality of other TCM preparations.}
}
