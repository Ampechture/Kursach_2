author = {Dominique J. Monlezun},
keywords = {Big Data, Multiomics, Personalized medicine, Pharmacogenetics, Radiogenomics},
abstract = {This chapter discusses the AI transformation of precision medicine (PrMed) by beginning with the distinction between precision versus personalized medicine, its historical development, and key trends (including genomic matching and umbrella, basket, and N-of-1 trials). It then moves on to clarify the central structural relationship in modern healthcare transformation between public health (PubHealth) and PrMed, to progressively build in this chapter on how AI is advancing (and posing new problems) for PrMed and its relationship with PubHealth (allowing the subsequent chapter's corollary discussion from the perspective of PubHealth). This chapter then considers the strategic and technical changes producing the system trend: AI+healthcare Big Data=PrMed. It then moves on to consider the concrete steps in this transformation including clarifying the clinical and organizational structures of healthcare Big Data (including data oceans), AI analytics, value-based approach to PrMed (including value barriers and countermeasures), and the impact of the above on the future's healthcare system design. It then considers AI-enabled multiomics in PrMed, including translational multiomics, pharmacogenetics, and radiogenomics use cases to eventually build to the value proposal of the emerging future's healthcare system: AI-enabled data science+multiomics=PrMed's future.}
}
@article{CATALA2022100987,
title = {Approaches for corneal endothelium regenerative medicine},
journal = {Progress in Retinal and Eye Research},
volume = {87},
pages = {100987},
year = {2022},
