@article{ZHOU2024117418,
title = {Traditional Chinese Medicine formula, Sanwujiao granule, attenuates ischemic stroke by promoting angiogenesis through early administration},
journal = {Journal of Ethnopharmacology},
volume = {321},
pages = {117418},
year = {2024},
issn = {0378-8741},
doi = {https://doi.org/10.1016/j.jep.2023.117418},
url = {https://www.sciencedirect.com/science/article/pii/S0378874123012886},
author = {Qinyang Zhou and Ji Ma and Qiuyan Liu and Changyue Wu and Ziwei Yang and Tingting Yang and Qimeng Chen and Yunyun Yue and Jing Shang},
keywords = {Traditional Chinese medicine formula, Ischemic stroke, Angiogenesis, Early administration, Molecular mechanism},
abstract = {Ethnopharmacological relevance
Ischemic stroke (IS) is one of the most lethal diseases with the insufficient pharmacology therapeutic approach. Sanwujiao granule (SW) is widely used for IS in China with little known about its underlying mechanism.
Aim of the study
To investigate the characteristics of therapeutic effects and potential mechanisms of SW against IS.
Materials and methods
The fingerprint of SW was applied by high-performance liquid chromatography-mass spectrometry (HPLC-MS). Three different drug treatment strategies, including prophylactic administration, early administration and delayed administration, were applied in rats' permanent middle cerebral occlusion (pMCAO) model. The Garcia neurological deficit test, adhesive removal test, rotarod test, TTC and TUNEL staining were performed to evaluate the pathological changes. The transcriptomic analysis was used to predict the potential mechanism of SW. The vascular deficiency model of Tg(kdrl:eGFP) zebrafish larvae and oxygen-glucose deprivation model on bEnd.3 cells were used to verify SW's pharmacological effect. qRT-PCR, immunofluorescent staining and Western Blot were applied to detect the expression of genes and proteins. The network pharmacology approach was applied to discover the potential bioactive compounds in SW that contribute to its pharmacological effect.
Results
SW early and delayed administration attenuated cerebral infarction, neurological deficit and cell apoptosis. The transcriptomic analysis revealed that SW activated angiogenesis-associated biological processes specifically by early administration. CD31 immunofluorescent staining further confirmed the microvessel intensity in peri-infarct regions was significantly elevated after SW early treatment. Additionally, on the vascular deficiency model of zebrafish larvae, SW showed the angiogenesis effect. Next, the cell migration and tube formation were also observed in the bEnd.3 cells with the oxygen-glucose deprivation induced cell injury. It's worth noting that both mRNA and protein levels of angiogenesis factor, insulin-like growth factor 1, were significantly elevated in the pMCAO rats' brains treated with SW. The network pharmacology approach was applied and chasmanine, karacoline, talatisamine, etc. were probably the main active compounds of SW in IS treatment as they affected the angiogenesis-associated targets.
Conclusions
These results demonstrate that SW plays a critical role in anti-IS via promoting angiogenesis through early administration, indicating that SW is a candidate herbal complex for further investigation in treating IS in the clinical.}
}
@article{SHIHHSUN2022S43,
title = {Inverse problem algorithm verification by integrated case studies in preventive medicine},
journal = {Journal of Medical Imaging and Radiation Sciences},
volume = {53},
number = {4, Supplement 1},
pages = {S43},
year = {2022},
note = {ISRRT conference proceedings},
issn = {1939-8654},
doi = {https://doi.org/10.1016/j.jmir.2022.10.140},
url = {https://www.sciencedirect.com/science/article/pii/S1939865422005197},
author = {Huang Shih-Hsun and Peng Bing-Ru and Lin ChihSheng and Tsai Hui-Chieh and Kittipayak Samrit and Pan Lung-Fa and Pan Lung-Kwang},
keywords = {inverse problem algorithm, risk factors, inverse matrix, numerical analysis, preventive medicine},
abstract = {Introduction
As a component of robust designation field, the IPA technique is reputed for providing a quantified index of expectation value from multiple patients' biological index series. The expectation value can be a specific drug in blood concentration reading, or the potential risk of a patient with a clinical syndrome, or the optimal trigger timing for CTA. This study verified the inverse problem algorithm (IPA) by integrated case studies in preventive medicine.
Methods
The risk factors can be Age, BSA (body surface area), Sugar AC, MAP (mean artery pressure), or others. The risk factor index is normalized to eliminate the physical dimensionality and yield a quantified index between -1.0 and +1.0, whereas the middle point, 0.0, implies the average risk factor index. The quantified indices of patients are organized into a vast data matrix for deriving the inverse matrix and obtaining the column matrix of the compromised solution. This study preset the STATISTICA program to derive the solution according to default quasi-Newton or Rosenbrock analysis.
Results
The derived solution can be defined as a nonlinear semi-empirical formula with multiple terms to express the specific expectation value. Accordingly, six IPA-related papers are comprehensively integrated to explore the applicability and critical points in adopting the IPA technique for preventive medicine tasks. Besides, the interpretation of cross-interactions among risk factors to affect the performing of IPA computation and the convergence rate of a semi-empirical formula related to the final constant term are also analyzed and discussed in detail.
Conclusion
The integrated IPA technique is instrumental in exploring potential syndromes in preventive medicine.}
}
