@article{BUSTOSCLARO202231,
title = {Relationship between internal medicine journals’ activity on social networks and the citations they receive},
journal = {Revista Clínica Española (English Edition)},
volume = {222},
number = {1},
pages = {31-36},
year = {2022},
issn = {2254-8874},
doi = {https://doi.org/10.1016/j.rceng.2020.10.008},
url = {https://www.sciencedirect.com/science/article/pii/S2254887421001624},
author = {M.M. Bustos-Claro and V. Ávila and D.G. Fernández-Ávila and Ó. Muñoz-Velandia and Á.A. García},
keywords = {Social networks, Facebook, Twitter, Internal medicine, Redes sociales, Facebook, Twitter, Medicina interna},
abstract = {Background and objectives
Social networks are a means for disseminating scientific information. Alternative metrics assessing the impact of scientific publications on social networks have been created. Our study aims to assess the correlation between the activity of internal medicine journals on social networks and traditional metrics based on citations.
Methods
Internal medicine journals were identified in the SCImago-Scopus database and information on traditional impact metrics was extracted. In addition, alternative metrics of activity were determined for Facebook, Twitter, YouTube, and Instagram. The correlation was assessed through Spearman's correlation coefficient.
Results
Of 134 Internal Medicine journals, 17 had a presence on the social networks evaluated. The SJR index was higher in journals with a presence on social networks vs. those without (59 vs. 18, p < .0001). The overall correlation between the SJR index and the number of followers/year was very strong for Facebook (Spearman's correlation coefficient 0.95, p < .05) and strong for Twitter (Spearman's correlation coefficient 0.54 p < .05).
Conclusions
Our study suggests that there is a very strong correlation between social network activity metrics (mainly Facebook and Twitter) compared to traditional metrics based on the number of citations of internal medicine journals.
Resumen
Antecedentes y objetivo
Las redes sociales son medios de divulgación de información científica. Se han generado métricas alternativas que evalúan el impacto de publicaciones científicas en redes sociales. Este estudio busca evaluar si existe correlación entre la actividad de las revistas de medicina interna en las redes sociales y las métricas tradicionales basadas en citaciones.
Métodos
Se identificaron las revistas de medicina interna a partir de la base de datos SCImago. Se extrajo información de las métricas tradicionales y alternativas de actividad de las revistas en Facebook, Twitter, YouTube e Instagram. Se evaluó correlación mediante el coeficiente de Spearman.
Resultados
De un total de 134 revistas de medicina interna, 17 contaban con presencia en las redes sociales evaluadas. El índice SJR fue más alto en revistas con red social vs. sin red social (59 vs. 18, p < 0,0001), y la correlación global entre el índice SJR y el número de seguidores/año en Facebook fue muy fuerte (coeficiente de Spearman 0,95, p < 0,05) y fuerte para el número de seguidores/año en Twitter (coeficiente de Spearman 0,54, p < 0,05).
Conclusiones
Nuestro estudio sugiere que existe una correlación muy fuerte entre las métricas de actividad de las redes sociales, comparado con las métricas tradicionales basadas en el número de citaciones en revistas de medicina interna.}
}
@article{LAWTON2023e267,
title = {#Friendtorship: Cultivating Peer Mentor Relationships in Hospice and Palliative Medicine (TH125A)},
journal = {Journal of Pain and Symptom Management},
volume = {65},
number = {3},
pages = {e267-e268},
year = {2023},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2022.12.059},
url = {https://www.sciencedirect.com/science/article/pii/S0885392422010922},
author = {Andrew J. Lawton and Bethany-Rose Daubman and Leah B. Rosenberg and April Zehm and Sudha Natarajan},
abstract = {Outcomes: 1. Define “friendtorship” and describe the potential benefits of this peer mentoring relationship for palliative medicine clinicians 2. Identify strategies to form and maintain friendtorships with peers in palliative medicine Cultivating mentoring relationships is a key component of professional development. Hospice and palliative medicine (HPM) clinicians risk burnout from the demands of emotionally intense clinical work and high expectations of scholarly productivity for many. Pressures are particularly applicable to female clinicians, individuals from underrepresented cultural groups, and those who act as primary caregivers for family members. To thrive, HPM clinicians require the support of effective mentors. However, the finite number of seasoned mentors and variability in mentor approaches and skill can make successful pairings challenging to establish. “Friendtorship,” a neologism of “friend-mentorship,” is a novel model of mentorship that may have unique benefits for HPM clinicians. Friendtors are peers with shared interests who enjoy a personal connection while also intentionally supporting each other's career development. When compared to traditional mentorship between novice and senior clinicians, friendtorships may be less hierarchical, allow for more honest and frequent dialogue, and invite more authentic discussion of personal and professional challenges. Friendtors can provide guidance on diverse topics, including clinical quandaries, professional opportunities, parenting challenges, and the vicissitudes of a partner's career. Such insights may be invaluable to HPM clinicians, as they face shifting personal and professional priorities throughout the career trajectory. In this session, a group of interdisciplinary HPM clinicians will describe how they have cultivated effective friendtorships. They will share how they innovated a friendtorship structure to nurture their professional growth and personal well-being. They will propose strategies for establishing one's own friendtor relationships, including formalizing relationships with admired peers, establishing mutual accountability and trust, and regularly checking in to discuss the impact and goals of the relationship. The group will center the mentorship needs of physician and non-physician HPM clinicians, as well as those of underrepresented backgrounds. Attendees will leave with tools to forge and maintain their own friendtorships in HPM.}
}
