@article{HARZANDJADIDI2023387,
title = {Adaptation of a European categorization system for driving-impairing medicines in Iran},
journal = {Traffic Injury Prevention},
volume = {24},
number = {5},
pages = {387-392},
year = {2023},
issn = {1538-9588},
doi = {https://doi.org/10.1080/15389588.2023.2203789},
url = {https://www.sciencedirect.com/science/article/pii/S1538958823000073},
author = {Sepideh {Harzand Jadidi} and Mostafa Farahbakhsh and Homayoun Sadeghi-Bazargani and Faramarz Pourasghar},
keywords = {Traffic accident, driving-impairing medicine, drug classification, driving under the influence (DUI), fitness to drive},
abstract = {Objective
Road traffic crashes due to impaired driving are a leading cause of preventable injuries and deaths. The purpose of this study was adaptation of a European categorization system for driving-impairing medicines in Iran.
Methods
DRUID categorization system was used as a leading model to classify medicines. Medicines that were compatible with DRUID categorization system were identified and classified accordingly. Medicines that were not compatible with DRUID categorization system were assessed in an expert panel in terms of possiblity of classification. Instructions for health care providers and advice for patients were prepared based on the medicine’s influence on fitness to drive.
Results
Of the 1255 medicines in Iranian pharmacopeia, 488 medicines were classified in four categories. Among classified medicines 43.85% and 25.41% belonged to Category 0 and Category 1. About 13.94%, 10.04%, and 6.76% pertained to Category 2, Category 3, and Multiple categories respectively. Majority of the medicines with moderate and severe adverse influences on driving fitness belonged to the nervous system medicines (72.65%). Most of the medicines with non-existing or minor adverse influences on driving fitness pertained to cardiovascular medicines (16.56%). Majority of uncategorized medicines belonged to Iranian herbal medicines.
Conclusion
The current study disclosed that DRUID categorization system was implementable for most of the commonly prescribed medicines. Experimental studies are needed to determine the influence of uncategorized medicines of Iranian pharmacopeia. Other countries with similar settings can adapt DRUID categorization system until they develop their own model using original studies.}
}
@article{OZER202429,
title = {Assembling a justified list of academic words in veterinary medicine: The veterinary medicine academic word list (VMAWL)},
journal = {English for Specific Purposes},
volume = {74},
pages = {29-43},
year = {2024},
issn = {0889-4906},
doi = {https://doi.org/10.1016/j.esp.2023.12.002},
url = {https://www.sciencedirect.com/science/article/pii/S0889490623000881},
author = {Mustafa Özer and Erdem Akbaş},
keywords = {English for veterinary medicine, Academic English, Academic word list, Discipline-specific academic vocabulary, Corpus linguistics},
abstract = {The assembly of corpus-based discipline-specific word lists for pedagogical purposes has recently been on the rise (e.g., Arndt, 2022; Fraser, 2007; O'Flynn, 2019; Yang, 2015). In order to cater to the needs of learners in veterinary medicine (VM) and for field-specific academic literacy, this paper analyses a reiteration of the Veterinary Medicine Corpus (Özer and Akbaş, 2023; hereafter the VMC), assembling a specified list of academic words used in published research articles (RAs) in VM. So far, VM has remained largely unexplored with the exception of Durrant's (2009) limited coverage of VM texts. The analysis was conducted using LancsBox 6.0, prompted to yield a list of PoS-tagged lemmas (P-lemmas) sorted by frequency (Coxhead, 2000; Yang, 2015) and dispersion (Arndt, 2022). The most frequent 1,000 lemmas for each content word category were collated into a candidate list. We then manually eliminated proper nouns and GSL (West, 1953) words. The final list, the Veterinary Medicine Academic Word List (VMAWL), contains 835 PoS-tagged lemmas (P-lemmas), a reiteration of which was produced by adding the types in Bauer and Nation’s (1993) taxonomy at the second level to test coverage. The VMAWL was profiled and validated against the four parent categories independently on AntWordProfiler (Anthony, 2022a) with analysis showing the VMAWL comprises 13.75 % of the VMC and diverges greatly from generic word lists like the AWL, NGSL, new-GSL, and AVL. The list can be used to develop teaching materials for EAP or ESP academic writing courses.}
}
@article{MURAJ2024S8,
title = {Precision Medicine or Personalized Medicine: Where does Radiotheranostics fit? A Scoping Review },
journal = {Journal of Medical Imaging and Radiation Sciences},
volume = {55},
number = {1, Supplement },
pages = {S8},
year = {2024},
note = {RTi3 2024 Conference Proceedings},
issn = {1939-8654},
doi = {https://doi.org/10.1016/j.jmir.2024.03.021},
url = {https://www.sciencedirect.com/science/article/pii/S1939865424000754},
author = {Zaynab Muraj and Tara Rosewall and Rebecca Wong and Andrea Shessel},
abstract = {Purpose
Radiation therapy has been described as both precise and personalized. Radiotheranostics is a novel treatment involving the use of radioisotopes for diagnostic and therapeutic purposes to bind to receptors on the tumor cells. Recent examples of radiotheranostics are Lutetium 177 DOTATATE that targets somatostatin receptors on neuroendocrine tumors and Lutetium 177 PSMA which attaches to prostate-specific membrane antigen [PSMA] in prostate cancer. The purpose of this review was to define personalized medicine, precision medicine, and to relate these concepts to radiotherapy and radiotheranostics for cancer treatment.
Methodology
Papers with the title terms precision medicine, personalized medicine and radiation or (Radio)theranostics were searched for in MEDLINE. There were no limitations on years, full-text availability or language. The most relevant papers were read and synthesized.
Results
Personalized medicine is defined as the use of an individual's genetic profile to guide decisions made about the prevention, diagnosis and treatment of disease. It is also suggested that shared decision making, patient satisfaction, engagement, compliance and patient centered outcomes reflect the ideals of personalized medicine. Precision medicine is defined as an emerging approach for disease treatment and prevention that accounts for individual variability in genes, but also the environment and lifestyle for each person. Both definitions include the use of genetics in the decisions for treatment. Personalized medicine may also refer to patient centered approaches. External beam radiation therapy is considered precise medicine for several reasons; however, the reasons are unrelated to genetics. In radiation medicine, there are two forms of precision, physical and biological. Biological precision or individual patient genetics are less frequently the dominant factor in the context of external beam radiation therapy, as volume and dose fractionation is guided by prognostic groupings rather than tailored to individual cancer features. Radiation therapy is considered precise from a physical point of view. Patient-specific tumor and normal tissue anatomy are contoured, and treatment plans are designed to maximize the therapeutic ratio. Also, advances in image guidance improve the technical precision of radiation treatment delivery. Radiotheranostics involves radiopharmaceuticals that are designed to bind to receptors on tumour cells to treat cancer after using diagnostic imaging to identify the target receptors. Compared with radiation therapy, radiotheranostics more closely fits the definition of precision medicine. Radiotheranostics adds precision medicine into the armamentarium of Radiation Medicine. One approach to personalization has been to modify dose (escalation or reduction) based on kidney sensitivity.
Conclusions
Personalized medicine and precision medicine are often used interchangeably; however, by understanding the definitions of precision medicine and personalized medicine, we can work on focusing our practices on both concepts. In the context of radiotheranostics, as treatment workflows and programs are developed, we seek to understand how the precise treatment of radiotheranostics can be more personalized. Opportunities for personalization lie in dose modification based on patient characteristics and in focusing on the patient experience.}
}
@article{NG2023101230,
title = {Artificial intelligence education: An evidence-based medicine approach for consumers, translators, and developers},
