@article{AHMED2022121256,
title = {Sticking - pulling strategy for assessment of combined medicine for management of tough symptoms in COVID-19 Pandemic using different windows of spectrophotometric Platform-Counterfeit products' detection},
journal = {Spectrochimica Acta Part A: Molecular and Biomolecular Spectroscopy},
volume = {277},
pages = {121256},
year = {2022},
issn = {1386-1425},
