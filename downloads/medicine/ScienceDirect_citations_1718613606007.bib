author = {Miao Liu and Wanzhen Xiao and Hong Zhang and Guoxiang Sun},
keywords = {Citri Reticulatae Pericarpium, Fingerprints, Quality consistency evaluation, Chemometric methods, Antioxidant activity},
abstract = {The study aimed to provide a reliable and feasible strategy for the comprehensive quality control of medicine food homology materials (MFHM). The high performance liquid chromatography (HPLC) fingerprints and Fourier transform mid-infrared (FT-MIR) quantized fingerprints were successfully developed to comprehensively evaluate overall quality of Citri Reticulata Pericarpium (CRP) by applying comprehensive quantified fingerprint method (CQFM). All samples were well distinguished and divided into 5 grades. In addition, through principal component analysis (PCA) and partial least squares discriminant analysis (PLS-DA), the identification ability of HPLC fingerprints and FT-MIR fingerprints on CRP with different storage years was discussed. The results showed that HPLC fingerprints combined with PCA had good discrimination ability, and the PLS-DA model established by the preprocessed FT-MIR fingerprint data could accurately distinguish and predict the storage period of CRP. Finally, based on 1, 1-diphenyl-2-picrylhydrazyl radical (DPPH•) scavenging assay, combined with bivariate correlation analysis, the fingerprint-activity relationship of offline antioxidant activity of CRP samples with the fingerprints peak were studied. In general, the comprehensive strategies provide a reliable and scientific reference scheme for the quality control of MFHM in the future.}
}
@article{REDDY2023e260,
title = {When Opposites Attract: The Development and Implementation of a Hybrid ICU and Palliative Care Rotation for Internal Medicine Residents (TH116B)},
journal = {Journal of Pain and Symptom Management},
volume = {65},
number = {3},
pages = {e260},
year = {2023},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2022.12.045},
url = {https://www.sciencedirect.com/science/article/pii/S0885392422010788},
author = {Padmaja Reddy and Matt Leveno and Heather A. Dalton and Lauren Ward and Eric A. Markinson and Rosechelle Ruggiero and Bethany Lussier},
abstract = {Outcomes: 1. Re-create the development process, structure, workflow, documentation/billing strategies, and curricular components of the hybrid ICU/palliative rotation described in this session 2. Apply the strategies outlined in the session to expand access to palliative care clinical experiences in a manner that will meet new ACGME requirements In July 2019, the Palliative Care team at UT Southwestern Medical Center created a 2-week-long clinical rotation for internal medicine residents. Although the rotation was well-received, residents universally commented that the experience was too short to allow for adequate practice of communication skills. Additionally, although they received ample exposure to conversations regarding late goals of care, they noted a dearth of opportunities to be observed and receive coaching on upstream communication and other primary palliative skills. Unfortunately, due to competing service requirements, our residency program was unable to increase the length of the palliative care rotation. In this session, we describe the innovative strategy employed to increase primary palliative skills training and bedside communication coaching at our institution—the development and implementation of a hybridized medical ICU/palliative care rotation in which residents rotating through the medical ICU are jointly supervised by a critical care attending and a palliative care attending. Palliative faculty augment bedside and didactic teaching in the ICU, and pulmonary and palliative faculty model collegial collaboration between specialties. We will describe typical daily workflow, present novel strategies employed for documentation and billing, and outline the didactic and bedside curricula that were developed for this unique educational experience. Residents are coached in assessing communication preferences, navigating cultural differences, collecting spiritual histories and responding to the language of faith, collaborating with interpreters to overcome language barriers, conducting virtual family meetings, managing symptoms and emotions in the context of discontinuation of artificial means of support, and much more. As the Accreditation Council for Graduate Medical Education recently instituted a mandate that every internal medicine resident receive clinical training in hospice and palliative medicine, our hope is that this session will give attendees a blueprint by which they can rapidly upscale exposure to palliative skills training at their home institutions.}
}
@article{KERET2023103314,
title = {Differentially expressed genes in systemic sclerosis: Towards predictive medicine with new molecular tools for clinicians},
journal = {Autoimmunity Reviews},
volume = {22},
number = {6},
pages = {103314},
year = {2023},
issn = {1568-9972},
doi = {https://doi.org/10.1016/j.autrev.2023.103314},
url = {https://www.sciencedirect.com/science/article/pii/S1568997223000484},
author = {Shiri Keret and Doron Rimar and Pauline Lansiaux and Erik Feldman and Alain Lescoat and Neta Milman and Dominique Farge},
keywords = {Systemic sclerosis, RNA gene expression, Machine learning, Autologous hematopoietic stem cell transplantation, Intrinsic subsets},
abstract = {Systemic sclerosis (SSc) is a rare and chronic autoimmune disease characterized by a pathogenic triad of immune dysregulation, vasculopathy, and progressive fibrosis. Clinical tools commonly used to assess patients, including the modified Rodnan skin score, difference between limited or diffuse forms of skin involvement, presence of lung, heart or kidney involvement, or of various autoantibodies, are important prognostic factors, but still fail to reflect the large heterogeneity of the disease. SSc treatment options are diverse, ranging from conventional drugs to autologous hematopoietic stem cell transplantation, and predicting response is challenging. Genome-wide technologies, such as high throughput microarray analyses and RNA sequencing, allow accurate, unbiased, and broad assessment of alterations in expression levels of multiple genes. In recent years, many studies have shown robust changes in the gene expression profiles of SSc patients compared to healthy controls, mainly in skin tissues and peripheral blood cells. The objective analysis of molecular patterns in SSc is a powerful tool that can further classify SSc patients with similar clinical phenotypes and help predict response to therapy. In this review, we describe the journey from the first discovery of differentially expressed genes to the identification of enriched pathways and intrinsic subsets identified in SSc, using machine learning algorithms. Finally, we discuss the use of these new tools to predict the efficacy of various treatments, including stem cell transplantation. We suggest that the use of RNA gene expression-based classifications according to molecular subsets may bring us one step closer to precision medicine in Systemic Sclerosis.}
}
