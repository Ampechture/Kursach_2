@article{ORTEGACHEN2023e618,
title = {Using a Palliative Extubation Simulation-Based Learning Model to Teach Key Palliative Medicine Topics and Skills Across Graduate and Undergraduate Medical Education (Sch472)},
journal = {Journal of Pain and Symptom Management},
volume = {65},
number = {5},
pages = {e618},
year = {2023},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2023.02.329},
url = {https://www.sciencedirect.com/science/article/pii/S0885392423004104},
author = {Christina Ortega-Chen and Andre Cipta},
abstract = {Outcomes
1. Self-report the ability to use a palliative extubation simulation curriculum to teach key palliative care topics to learners across various levels. 2. Self-report the ability to mitigate communication and procedural errors associated with suboptimal palliative extubations to provide maximum comfort for patients and families at end of life.
Background
The COVID-19 pandemic revealed gaps in the provision of end-of-life (EOL) care. In particular, the high number of mechanically ventilated patients at the EOL highlighted the need to standardize evidence-based palliative extubation (PE) amidst significant practice variation. Suboptimal PE practices can worsen symptoms, inappropriately hasten death, and add undue stress on patients, families, and healthcare staff. Effective training models are needed to standardize evidence-based PE. Simulation-based learning (SBL) has been shown to effectively improve medical knowledge, procedural proficiency, and interprofessional communication, all of which are necessary to successfully perform a PE. Unfortunately, the use of SBL in palliative care (PC) education is sparse.
Methods
An innovative educational activity using high-fidelity simulation and SBL models was created to teach key PC topics and skills within the context of a simulated PE procedure. The curriculum consists of a 1-hour didactic followed by 2 hours of simulation, divided into 4 sections. The simulation is designed to teach complex symptom management, communication strategies, and leadership skills. Each section concludes with a debrief to provide feedback and teaching points. This curriculum was piloted over multiple years among palliative medicine fellows. It will expand to include medical students this academic year with the plan to be further incorporated into medical resident education in the future. The following validated questionnaires will be used to study the efficacy of this SBL model: Student Satisfaction and Self-Confidence in Learning Scale, Simulation Design Scale, and Educational Practices Questionnaire. Participants will complete the questionnaires following the simulation.
Conclusion
Multiple specialties have adopted SBL as an effective modality to enhance training. The use of SBL in palliative medicine education is relatively sparse. An innovative PE-simulation educational activity may provide an optimal way to teach key PC concepts across graduate and undergraduate medical education.}
}
@article{DUSSAULT2023e600,
title = {Internal Medicine Resident Barriers to Serious Illness Conversations in the Primary Care Continuity Clinic (Sch440)},
journal = {Journal of Pain and Symptom Management},
volume = {65},
number = {5},
pages = {e600-e601},
year = {2023},
issn = {0885-3924},
doi = {https://doi.org/10.1016/j.jpainsymman.2023.02.177},
url = {https://www.sciencedirect.com/science/article/pii/S0885392423002567},
author = {Nicole Dussault and Elissa R. Nickolopoulos and Katherine Henderson and Patrick Hemming and Alex H. Cho and Jessica E. Ma},
abstract = {Outcomes
1. Self-report the ability to describe what barriers residents perceive to having serious illness conversations in the outpatient setting. 2. Self-report the ability describe what interventions residents perceive to be most useful to increase their ability to have serious illness conversations in the outpatient setting.
Background
While primary care providers regularly engage patients in serious illness conversations (SIC), it is not well known what specific challenges resident physicians face to achieving this Accreditation Council for Graduate Medical Education (ACGME) core competency. This project assessed resident perceptions of barriers to conducting outpatient SIC as well as potential interventions to address these barriers.
Methods
We distributed an electronic survey to all categorical internal medicine and medicine-psychiatry residents at our institution. Questions addressed outpatient SIC barriers and potential interventions in several domains: structural issues, personal knowledge, and communication skills. We reported results using descriptive statistics and Wilcoxon rank-sum tests, comparing responses by year in training (interns vs second and third years). Likert-scale responses were dichotomized to a “not at all or slightly” versus “moderate or extreme” barrier or helpful intervention.
Results
Out of 149 residents, 71 completed the survey (response rate 48%). Highest-scoring barriers to having outpatient SIC were at the structural level, relating to (1) lack of time in clinic (99%), (2) need to prioritize other medical problems (94%), and (3) lack of continuity (62%). Discomfort with communication frameworks was not reported as a major barrier (21%). Highest-scoring interventions included the ability to schedule dedicated SIC visits with themselves (96%) or with another provider (82%). When comparing interns to second and third years, interns were significantly less confident in their ability to have SICs and more likely to report lack of knowledge areas (ie, not understanding SIC components, patient prognosis, or how to fill out paperwork, p<0.05).
Conclusions
Residents report significant structural barriers to SIC conversations, such as limitations in time, continuity, and competing medical priorities, that may warrant greater program attention to interventions such as clinic schedules and workflow. Additional trainings may be most beneficial if targeted to interns at the beginning of their medical training.}
}
@article{BAJONAID2022e122,
