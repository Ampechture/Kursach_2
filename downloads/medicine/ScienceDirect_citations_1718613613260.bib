@incollection{LIU2022657,
title = {31 - A sound comprehension of molecular biology and relevant biotechnology is a prerequisite for research on the molecular mechanisms of traditional Chinese medicine, mainly medicinal herbs},
editor = {Maryam Sarwat and Hifzur Siddique},
booktitle = {Herbal Medicines},
publisher = {Academic Press},
pages = {657-673},
year = {2022},
isbn = {978-0-323-90572-5},
doi = {https://doi.org/10.1016/B978-0-323-90572-5.00020-2},
url = {https://www.sciencedirect.com/science/article/pii/B9780323905725000202},
author = {Zhengqi Liu and Tao Xu and Li Peng and Zhaozhao Hua and Jian Li and Zhigang Jiang and Dezhong Joshua Liao},
keywords = {Traditional Chinese medicine, Molecular biology, Biotechnology, Signaling transduction},
abstract = {Traditional Chinese medicine (TCM) plays an indispensable role in medical care in China. However, most of its therapeutic approaches, like various medicinal herb recipes, lack impeccable evidence from rigorously designed laboratory studies and clinical trials for their effectiveness, adversity, and underlying mechanisms. In recent decades the Chinese government has injected prodigious funding into TCM research, especially into those projects that dissect how medicinal herbs affect the expression of genes along one or more molecular signaling pathways to achieve their effects. However, most researchers in the funded projects are TCM practitioners who lack training in the theory and techniques of molecular biology. For example, reverse transcription and polymerase chain reactions are routinely used techniques in TCM research, but few of the researchers know well the principles and pitfalls of these techniques. As a result, the design and performance of the experiments, as well as the explanations and conclusions on the resulting data have often become porous, fallible, or biased. This worrisome trend needs to be corrected by tight collaborations among TCM practitioners, western medicine experts, and molecular biology pundits on the experimental design and performance and on interpretations of the resulting data.}
}
@article{THOMPSON2022322,
title = {Using crowdsourced medicine to manage uncertainty on Reddit: The case of COVID-19 long-haulers},
journal = {Patient Education and Counseling},
volume = {105},
number = {2},
pages = {322-330},
year = {2022},
issn = {0738-3991},
doi = {https://doi.org/10.1016/j.pec.2021.07.011},
url = {https://www.sciencedirect.com/science/article/pii/S0738399121004705},
author = {Charee M. Thompson and Kayla B. Rhidenour and Kate G. Blackburn and Ashley K. Barrett and Sara Babu},
keywords = {COVID-19, Crowdsourcing, Uncertainty management, Social media},
abstract = {Objective
Causes of and treatments for long-COVID syndrome remain unknown. Drawing on uncertainty management theory (UMT), this study elucidates the communicative nature of crowdsourced medicine as a means by which COVID “long-haulers” respond to their poorly understood illness.
Methods
31,892 posts on the long-haulers subreddit (r/covidlonghaulers) were analyzed, starting with its creation date, July 24th, 2020, until January 7, 2021. The Meaning Extraction Method was used to identify clusters of words that mathematically group together across the text observations.
Results
Analyses yielded 16 distinct factors of words, which we thematized based on their composition, the data, and UMT. The 16 themes encompassed symptoms (e.g., pain, respiratory, sensory), diagnostic concerns (testing, diagnosis), broad health concerns (immunity, physical activity, diet), chronicity, support, identity, and anxiety.
Conclusion
Findings provide a succinct, yet robust set of themes reflecting the information-seeking (i.e., “This is happening to me”) and support-seeking functions of long-haulers’ talk (i.e., “Is this happening to you?”). Findings have implications for collective uncertainty management, online crowdsourcing, and patient advocacy.
Practice implications
We recommend that health care providers employ sensitivity when addressing the anxiety that long-haulers are experiencing while also validating that their physical symptoms are real. Online communities help long-haulers manage their uncertainty.}
}
