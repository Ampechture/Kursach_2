@article{SUMMIT2023110195,
title = {P026 - A NATIONAL SNAPSHOT OF ABORTION TRAINING IN FAMILY MEDICINE RESIDENCY PROGRAMS},
journal = {Contraception},
volume = {127},
pages = {110195},
year = {2023},
issn = {0010-7824},
doi = {https://doi.org/10.1016/j.contraception.2023.110195},
url = {https://www.sciencedirect.com/science/article/pii/S0010782423002895},
author = {AK Summit and E Chong},
abstract = {Objectives
While early abortion is well within the scope of family medicine, it is not routinely taught in family medicine residency programs (FMRPs). We surveyed FMRP directors nationally to learn more about variations in reproductive health training.
Methods
Our survey questions were part of a larger online survey conducted by the Council of Academic Family Medicine Educational Research Alliance (CERA), fielded to directors of all known US FMRPs in Fall 2022. Analysis examined descriptive statistics and included chi-square tests to assess whether differences among programs and program directors were significantly related to the abortion training available. Binary logistic regression was used for multivariate analysis.
Results
The overall response rate was 42% (286/678). Of programs in the sample, 19% offered routine training and 58% offered elective training in medication abortion, and 10% and 52% offered elective and routine training in procedural abortion, respectively. In the multivariate model, lower state abortion restrictiveness levels, larger program size, non-Catholic affiliation, and belief of the program director about abortion training were significantly associated with routine abortion training. Fifty-three programs (30% of those with any abortion training) allowed abortion provision in the continuity clinic or other family medicine setting.
Conclusions
Among responding FMRPs, 19% offered at least one type of routine abortion training. While training availability has increased since similar research was conducted in 2011, most training is elective, and is still grossly inadequate to prepare family physicians to meet their patients’ full reproductive health needs, especially in the face of the current crisis in abortion access in the US.}
}
@article{YOON2022100893,
title = {Comparison of the acupuncture manipulation properties of traditional East Asian medicine and Western medical acupuncture},
