@incollection{DELGADO2024133,
title = {Chapter 9 - Beyond one-size-fits-all: Precision medicine and novel technologies for sex- and gender-inclusive COVID-19 pandemic management},
editor = {Simona Mellino and Dario Motti and Enrico Santus and Antonella {Santuccione Chadha}},
