@article{CHEGANI2023e40,
title = {Artificial Intelligence for Rapid Clinical Diagnosis in Oral Medicine},
journal = {Oral Surgery, Oral Medicine, Oral Pathology and Oral Radiology},
volume = {135},
number = {2},
pages = {e40-e41},
year = {2023},
issn = {2212-4403},
doi = {https://doi.org/10.1016/j.oooo.2022.09.023},
url = {https://www.sciencedirect.com/science/article/pii/S2212440322011506},
author = {JOULIANA DAVOUDI CHEGANI and MICHAEL ASADOORIAN and PARISH P. SEDGHIZADEH},
abstract = {Objectives: Delayed diagnosis of oral pathology, such as oral cancer or immune mediated conditions, can result in significant morbidity and in some cases mortality. The purpose of this study and method was to improve clinical diagnosis by providing a simple Artificial Intelligence (AI) tool to increase efficiency and accuracy in clinical diagnostics, while reducing misdiagnosis. We used a classification scheme for oral lesion image classification based on deep convolutional neural networks (CNN). Convolutional networks are considered the state-of-the-art technique for classification problems when the input is high-dimensional data such as images. At the first training batch we used two lesions in lichen planus, mucocele, for proof of concept, our testing model achieved an accuracy of 84.9% in lichen planus recognition and 76.02% for mucocele. The purpose of this study was to create and apply an Artificial Intelligence tool (Oromed AI®) to aid in oral medicine diagnostics. For proof of concept to train the AI system and show it can work we chose mucocele and lichen planus and then we will add additional conditions. Methods: Our dataset comprised 1089 high and medium resolution images of oral lesions consisting of 558 labeled as mucocele and 531 as lichen planus. These images are taken by clinical experts in oral medicine and pathology with training in intraoral photography. Images for which there was a disagreement between pathologists were discarded. We used a classification scheme for the clinical images of oral lesions based on deep convolutional neural networks (CNN). Results: Convolutional networks are considered the state-of-the-art technique for classification of problems when the input is high-dimensional data such as images, and CNN proved useful in our AI modeling. Our testing model and Oromed AI software achieved an accuracy of 84.9% in lichen planus recognition and 76.02% for mucocele. Conclusions: AI can aid in oral medicine diagnosis by providing rapid real-time analyzing capability to assist clinicians in diagnostics. AI is not meant to replace gold standard diagnosis such as biopsy and histopathologic examination, but rather serve as a clinical adjunct or tool for aiding in initial differential diagnosis.}
}
@article{CHO2022102664,
title = {Tele-medicine question response service: Analysis of benefits and costs},
