@article{LOPUS2023117,
title = {Nano-ayurvedic medicine and its potential in cancer treatment},
journal = {Journal of Integrative Medicine},
volume = {21},
number = {2},
pages = {117-119},
year = {2023},
issn = {2095-4964},
