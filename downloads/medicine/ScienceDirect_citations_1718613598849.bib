@article{ZHOU2022101269,
title = {Effects of dietary fermented Chinese herbal medicines on growth performance, digestive enzyme activity, liver antioxidant capacity, and intestinal inflammatory gene expression of juvenile largemouth bass (Micropterus salmoides)},
journal = {Aquaculture Reports},
volume = {25},
pages = {101269},
year = {2022},
issn = {2352-5134},
