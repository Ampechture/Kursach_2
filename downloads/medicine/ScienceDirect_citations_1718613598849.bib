@article{BHAT2024S142,
title = {IMPROVING PRODUCTIVITY AND REDUCING COGS USING DIELECTRIC SPECTROSCOPY AS A PAT TOOL TO MEASURE LIVE BIOMASS IN REGENERATIVE MEDICINE, CELL & GENE THERAPY, VACCINE/VIRAL VECTOR AND OTHER CELL CULTURE APPLICATIONS},
journal = {Cytotherapy},
volume = {26},
number = {6, Supplement },
pages = {S142},
year = {2024},
note = {Abstracts of the 30th Annual ISCT Meeting May 29-June 1, 2024},
issn = {1465-3249},
doi = {https://doi.org/10.1016/j.jcyt.2024.03.276},
url = {https://www.sciencedirect.com/science/article/pii/S1465324924003645},
author = {A. Bhat and J. Poppleton},
keywords = {Cell therapy, Live cells, Capacitance},
abstract = {Background & Aim
Following FDA's PAT initiative, it is imperative that critical process parameters (CPP) that significantly impact critical quality attributes (CQA's) of a process are monitored and controlled strictly. It is also well known that viable biomass is one such CPP, or a Key Performance Indicator, which can impact CQA's in a bioprocess. Dielectric spectroscopy, referred to as capacitance measurement, has been used for monitoring biomass in bioprocesses in real time routinely over the past two decades. This technology has been used in the industry for applications ranging from batch fingerprinting, troubleshooting, optimization of nutrient feed, viral infection, cell density control, scale up, among others. Owing to its success in bioprocess applications, dielectric spectroscopy has also been used in regenerative medicine applications and monitoring cell density in 3D tissues. Moreover, this technology has resulted in significant improvement in productivity and reduction in COGS in these applications.
Methods, Results & Conclusion
This presentation will cover the use of capacitance technology to measure CAR-T cells in real time, while identifying real time growth trend, activation, expansion, media exchange and dilutions. Another study will be presented that utilizes this technology for monitoring PBMC's in a flow through cell. Measurement settings and flow rates were optimized for this work, which can be used for similar studies in the future. Capacitance was also used to measure T cells from different donors. The work presented wil indicate that capacitance probes may not need to be calibrated differently for cells derived from different donors. Finally, studies will be presented to demonstrate the use of this technology to reduce media costs up to 40% (in a perfusion application) and improve productivity 40 fold (in vaccine production).}
}
@article{RIZK2022111444,
title = {In vivo antibabesial activity and bioinformatic analysis of compounds derived from the Medicines for Malaria Venture box against Babesia microti},
